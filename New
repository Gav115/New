-- Scripts:

local function ZDXCQ_fake_script() -- PushUp.Script 
	local script = Instance.new('Script', PushUp)

	PushUp = script.Parent --assigned varible
	
	
	PushUp.MouseButton1Click:Connect(function()
			game:GetService("ReplicatedStorage").Remotes.PushUp:FireServer()
	end)
	
	
	PushUp.Parent.Ran.Visible = true wait(2) PushUp.Parent.Ran.Visible = false
end
coroutine.wrap(ZDXCQ_fake_script)()
local function RWIV_fake_script() -- SitUp.Script 
	local script = Instance.new('Script', SitUp)

	SitUp = script.Parent --assigned varible
	
	
	SitUp.MouseButton1Click:Connect(function()
		game:GetService("ReplicatedStorage").Remotes.SitUp:FireServer()
	end)
	
	
	SitUp.Parent.Ran2.Visible = true wait(2) SitUp.Parent.Ran2.Visible = false
end
coroutine.wrap(RWIV_fake_script)()
local function VILJP_fake_script() -- Dumbell.Script 
	local script = Instance.new('Script', Dumbell)

	Dumbell = script.Parent --assigned varible
	
	
	Dumbell.MouseButton1Click:Connect(function()
		game:GetService("ReplicatedStorage").Remotes.Dumbell:FireServer()
	end)
	
	
	Dumbell.Parent.Ran2.Visible = true wait(2) Dumbell.Parent.Ran2.Visible = false
end
coroutine.wrap(VILJP_fake_script)()
local function QCVCFQT_fake_script() -- Open.Script 
	local script = Instance.new('Script', Open)

	Button = script.Parent --[can change button to whatever you want your variable to be, make sure to change button.mousebutton one click to new 
	--[varible and everything else with button
	
	Button.MouseButton1Click:Connect(function()
		Button.Parent.FrameName.Visible = not Button.Parent.FrameName.Visible
	end)
	
	
	
	
	
	--[INFORMATION]
	--Mousebutton1click means when clicked
	--mousebutton1click:connect(function() | That means When the player clicks the first mouse button, it will conect to the function and run it every time per click.
	
end
coroutine.wrap(QCVCFQT_fake_script)()
